				"required": [
					"cond",
					"valueset"
				],
				"properties": {
					"cond": {
						"type": "array",
						"items": {
							"type": "string",
							"examples": [
								"|=|s:gvar@your_global_var"
							]
						}
					},
					"valueset": {
						"type": "object"
					}
				}
			}
		},
