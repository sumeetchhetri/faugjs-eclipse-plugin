	"$schema": "http://json-schema.org/draft-07/schema#",
	"additionalProperties": false,
	"type": "object",
	"title": "The FaugJs Schema Schema",
	"required": [
		"type",
		"properties",
		"details"
	],
	"properties": {
		"additionalProperties": false,
		"templatize": {
			"type": "boolean",
			"default": false,
			"examples": [
				true
			]
		},
		"type": {
			"type": "string",
			"examples": [
				"object"
			]
		},
		"dynamicAspects": {
			"type": "array",
			"items": {
				"type": "object",
